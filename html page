		function outfocusforfirstname(x){
			var firstname =document.getElementById('firstname');
			if(firstname.value !=""){
			x.classList.remove("border-danger");
			document.getElementById('form_error_first_name').classList.add("d-none");
			}else{
				x.classList.add("border-danger");
			document.getElementById('form_error_first_name').classList.remove("d-none");
			}
		}
		function outfocusforlastname(x){
			var lastname =document.getElementById('lastname');
			if(lastname.value !=""){
			x.classList.remove("border-danger");
			document.getElementById('form_error_last_name').classList.add("d-none");
			}else{
				x.classList.add("border-danger");
			document.getElementById('form_error_last_name').classList.remove("d-none");
			}
		}
		function outfocusforemail(x){


			var emailinput =document.getElementById('emailinput');
			if( /^\w+@[a-zA-Z_]+?\.[a-zA-Z]{2,4}$/.test(emailinput.value)){
			x.classList.remove("border-danger");
			document.getElementById('form_error_email').classList.add("d-none");
			}else{
				x.classList.add("border-danger");
			document.getElementById('form_error_email').classList.remove("d-none");
			}
		}
		function outfocusforphonenumber(x){
			var phonenumber =document.getElementById('phonenumber');
			if(phonenumber.value !=""){
			x.classList.remove("border-danger");
			document.getElementById('form_error_phone').classList.add("d-none");
			}else{
				x.classList.add("border-danger");
			document.getElementById('form_error_phone').classList.remove("d-none");
			}
		}
		function outfocusformessage(x){
			var messageinput =document.getElementById('messageinput');
			if(messageinput.value !=""){
			x.classList.remove("border-danger");
